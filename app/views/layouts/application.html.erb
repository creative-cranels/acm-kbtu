<!DOCTYPE html>
<html>
<head>
  <title>AcmKbtu</title>
  <%= stylesheet_link_tag  "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>

<div class="main">
  <%= render 'layouts/menu' %>

  <%- if notice != nil %>
  <p class="alert alert-info"><%= notice %></p>
  <%- end %>
  <%- if alert != nil %>
    <p class="alert alert-error"><%= alert %></p>
  <%- end %>

  <div class="container">
  <%= yield %>
  <div>
</div>
</body>
</html>
